pkgname=brute12
pkgver=1
pkgrel=5
pkgdesc='A tool designed for auditing the cryptography container security in PKCS12 format.'
url='http://www.security-projects.com/?Brute12'
groups=('cynuxsecurity' 'cynuxsecurity-windows' 'cynuxsecurity-cracker' 'cynuxsecurity-crypto')
license=('custom:unknown')
arch=('any')
makedepends=('git')
source=("git+https://github.com/CynuxSecurity/$pkgname.git")
sha512sums=('SKIP')

package() {
  install -dm 755 "$pkgdir/usr/share/windows/$pkgname"

  cp -a * "$pkgdir/usr/share/windows/$pkgname"
}

